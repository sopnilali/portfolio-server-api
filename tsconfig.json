    "module": "commonjs",
    "outDir": "./dist",   
    "rootDir": "./src",
    "moduleResolution": "node",
    "target": "ES2023",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "ignoreDeprecations": "5.0"
  },
  "include": ["src/**/*"],
  "exclude": ["prisma.config.ts"]
}